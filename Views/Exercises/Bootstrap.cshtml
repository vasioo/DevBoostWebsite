@using DevBoost.Data;
@model List<Exercise>

@{
    ViewData["Title"] = "InnerController";
}
<h2 class="text-center">Bootstrap Exercises Page</h2>
<hr />

<div class="col-md-4">
    <div class="card mb-3">
        <ul>
            @foreach (var item in Model)
            {
                @if (@item.Category == "7")
                {
                            <li>@item.Title</li>
                }
            }
        </ul>
    </div>
</div>
